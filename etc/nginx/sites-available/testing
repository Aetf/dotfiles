# Default server configuration
#
server {
	listen 80;
	server_name testing.unlimitedcodeworks.xyz;

	root /var/lib/tomcat8/webapps/ROOT;

	# Add index.php to the list if you are using PHP
	index index.jsp index.html index.htm index.nginx-debian.html;

	access_log /var/log/nginx/sites/testing.access netdata;
	error_log /var/log/nginx/sites/testing.error error;

	location / {
		proxy_pass http://127.0.0.1:8009;
	}
}
