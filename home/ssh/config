# Ansible managed, src: /home/aetf/.local/spec/roles/dotfiles/templates/home/ssh/config.j2

Host xps
    User aetf
    HostName aetf-arch-xps.zt.unlimited-code.works
    Port 59901
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

Host mcsh
    User root
    HostName mcsh.unlimited-code.works
    Port 8020
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
    ControlPersist yes

