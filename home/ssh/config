Match user cc
    HostName 192.5.87.%h
Match final host 192.5.87.*
    User cc
    UserKnownHostsFile /dev/null
    ForwardAgent yes
    ControlMaster auto
    ControlPath ~/.ssh/masters/%n:%r@%h:%p
